{
    "name": "papyrus/symfony-serializer",
    "description": "Implementation of papyrus/serializer, based on symfony/serializer",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "symfony-serializer",
        "serializer",
        "event-store",
        "event-sourcing",
        "ddd",
        "domain-driven-design",
        "cqrs",
        "papyrus"
    ],
    "authors": [
        {
            "name": "Jeroen de Graaf",
            "email": "hello@jero.work"
        }
    ],
    "require": {
        "php": "^8.1",
        "papyrus/serializer": "^0.2",
        "symfony/serializer": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.11",
        "maglnet/composer-require-checker": "^4.2",
        "phpro/grumphp-shim": "^1.13",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "scrutinizer/ocular": "^1.9",
        "symfony/property-access": "^6.1"
    },
    "provide": {
        "papyrus/serializer-implementation": "0.2"
    },
    "autoload": {
        "psr-4": {
            "Papyrus\\SymfonySerializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Papyrus\\SymfonySerializer\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "phpcsfixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}
